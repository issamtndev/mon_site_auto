{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Auto</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ entity.titre }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Nbporte</th>
                <td>{{ entity.nbporte }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ entity.prix }}</td>
            </tr>
            <tr>
                <th>Nbcylindre</th>
                <td>{{ entity.nbcylindre }}</td>
            </tr>
            <tr>
                <th>Energie</th>
                <td>{{ entity.energie }}</td>
            </tr>
            <tr>
                <th>Puissance</th>
                <td>{{ entity.puissance }}</td>
            </tr>
            <tr>
                <th>Boite</th>
                <td>{{ entity.boite }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('auto') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('auto_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
